{
  "name": "verza-scripts",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "verza",
    "build": "tsc --p tsconfig.prod.json && verza build",
    "start": "verza start",
    "server": "ts-node-esm ./src/server/script.ts",
    "serve": "serve dist -p 8085 --cors",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix,tsx --report-unused-disable-directives --max-warnings 0",
    "ts-check": "tsc --pretty --noEmit --p tsconfig.prod.json",
    "prepare": "husky install",
    "pre-commit": "npm run lint:fix && npm run ts-check"
  },
  "dependencies": {
    "@verza/sdk": "^2.0.51-dev.60",
    "clsx": "^2.0.0",
    "deep-equal": "^2.2.2",
    "leva": "^0.9.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.2",
    "@types/node": "^20.6.4",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "events": "^3.3.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "socket.io-client": "^4.7.2",
    "socket.io-msgpack-parser": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "three": "^0.156.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typescript": "^5.0.2",
    "uuid": "^9.0.1",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^4.0.0",
    "zod": "^3.22.2"
  }
}
